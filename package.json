    "@tabler/icons-react": "^3.37.1",
    "@tailwindcss/vite": "^4.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "effector": "^23.4.4",
    "effector-react": "^23.3.0",
    "effector-storage": "^7.1.0",
    "history": "^5.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
